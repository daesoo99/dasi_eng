          "practice_examples": [
            "I am happy",
            "I am tired", 
            "I am ready"
          ]
        },
        {
          "original_sentence": "Truth be told I never graduated from college",
          "basic_pattern": "I never + 동사과거형",
          "practice_examples": [
            "I never went there",
            "I never saw it",
            "I never tried it"
          ]
        },
        {
          "original_sentence": "Today I want to tell you three stories",
          "basic_pattern": "I want to + 동사원형",
          "practice_examples": [
            "I want to go",
            "I want to eat",
            "I want to sleep"
          ]
        },
        {
          "original_sentence": "I dropped out of Reed College",
          "basic_pattern": "I + 동사과거형",
